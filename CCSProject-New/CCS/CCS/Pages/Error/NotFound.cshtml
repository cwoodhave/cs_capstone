@page
@model CCS.Pages.Error.NotFoundModel
@{
    ViewData["Title"] = "Not Found";
}

<div class="container">
    <div class="row">
        <div class="col-8 offset-2">
            <div class="card">
                <div class="card-header bg-warning text-center">
                    <h2><span class="fa fa-exclamation-triangle"></span> ERROR 404: Page Not Found</h2>
                </div>
                <div class="card-body text-center">
                    Please make sure the page you are looking for exists.
                </div>
            </div>
        </div>
    </div>
</div>

